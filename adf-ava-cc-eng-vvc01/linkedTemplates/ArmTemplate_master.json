{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ava-cc-eng-vvc01"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"ls_DataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_DataLakeStorage'"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsavaccengvvc01.dfs.core.windows.net/"
		},
		"ls_AzureSqlDatabase_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sn-azsql-pwd-vvc"
		},
		"ls_DataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsavaccengvvc01.dfs.core.windows.net/"
		},
		"ls_akv_cc_eng_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-ava-cc-eng-vvc.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ava-cc-eng-vvc01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"ls_DataLakeStorage_accountKey": {
						"value": "[parameters('ls_DataLakeStorage_accountKey')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"ls_DataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_DataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_akv_cc_eng_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_cc_eng_properties_typeProperties_baseUrl')]"
					},
					"ls_AzureSqlDatabase_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_AzureSqlDatabase_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}